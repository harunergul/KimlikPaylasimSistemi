			<from-outcome>ayarlar</from-outcome>
			<to-view-id>/ayarlar.jsp</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>menu</display-name>
		<from-view-id>/menu.jsp</from-view-id>
		<navigation-case>
			<from-outcome>kisiSorgula</from-outcome>
